import java.io.*;
import java.util.*;
public class number
{
	public static void main(String args)
	{
		int n;
		Scanner s=new Scanner(system.in);
		n=s.nextInt();
		if (n==0)
		System.out.println(n +"is zero");
		else
		if (n>0)
		System.out.println(n+"is positive");
		else
		if(n<0)
		System.out.println(n+"is negative");
	}
}
